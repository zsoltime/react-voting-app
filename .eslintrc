{
  "env": {
    "browser": true,
    "mocha": true,
  },
  "extends": "airbnb",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    }
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "comma-dangle": ["error", {
      "arrays": "always-multiline",
      "exports": "never",
      "functions": "ignore",
      "imports": "never",
      "objects": "always-multiline",
    }],
    "import/extensions": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-unresolved": 0
  }
}
